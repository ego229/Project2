<h1>Edit InStock</h1>

<form action="/product/{{singleInStock._id}}?_method=PUT" method="POST">
    <input type="text" name="name" placeholder="name" value="{{singleProduct.name}}"/>
    <input type="text" name="version" placeholder="version" value="{{singleInStock.version}}"/>
    <input type="text" name="model" placeholder="model" value="{{singleInStock.model}}"/>
    

    <input type="submit" value="Save"/>
